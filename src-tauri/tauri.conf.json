{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "tauri-app",
	"version": "0.0.13",
	"identifier": "io.nordstud.tauri-app",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "pnpm build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "tauri-app",
				"width": 800,
				"height": 600
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": ["app", "dmg", "msi", "appimage", "deb", "rpm"],
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"createUpdaterArtifacts": true
	},
	"plugins": {
		"updater": {
			"pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDc0NDA0REI3NjczRTY3RTkKUldUcFp6NW50MDFBZEVSNEVkOFNDbEhETEdkWUFDTjNKQ2VCSmJYYkllRFlRVXJZcDFUN01uUlgK",
			"endpoints": ["http://localhost:3000/json"],
			"dangerousInsecureTransportProtocol": true
		}
	}
}

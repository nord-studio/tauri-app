{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
		"core:default",
		"opener:default",
		"updater:allow-check",
		"updater:allow-download-and-install",
		"updater:allow-download",
		"updater:allow-install",
		"updater:default",
		"dialog:default",
		"process:default",
		"process:allow-restart"
  ]
}